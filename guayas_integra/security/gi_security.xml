    <record id="group_guayas_fisioterapeuta" model="res.groups">
        <field name="name">Fisioterapeuta</field>
        <field name="category_id" ref="module_category_guayas_integra_operativo"/>
    </record>

    <record id="group_guayas_terapeuta" model="res.groups">
        <field name="name">Terapeuta</field>
        <field name="category_id" ref="module_category_guayas_integra_operativo"/>
    </record>

</odoo>