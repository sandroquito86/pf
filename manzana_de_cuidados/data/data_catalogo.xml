<odoo>
    <data noupdate="1">
        <!-- Catálogos -->
        <record id="tipo_servicio" model="mz.catalogo">
            <field name="name">Tipo de Servicio</field>
            <field name="descripcion">Catálogo de Tipo de Servicios</field>
        </record>
        

       
        <!-- Tipo de servicios -->
        <record id="tipo_servicio_cuidado" model="mz.items">
            <field name="name">Cuidado</field>
            <field name="descripcion">Tipo de Servicio Cuidado</field>
            <field name="catalogo_id" ref="tipo_servicio"/>
        </record>
        <record id="tipo_servicio_bienestar" model="mz.items">
            <field name="name">Bienestar  y Autocuidado</field>
            <field name="descripcion">Tipo de Servicio Bienestar  y Autocuidado</field>
            <field name="catalogo_id" ref="tipo_servicio"/>
        </record>
        <record id="tipo_servicio_formacion" model="mz.items">
            <field name="name">Formación y generación de ingresos</field>
            <field name="descripcion">Tipo de Servicio Formación y generación de ingresos</field>
            <field name="catalogo_id" ref="tipo_servicio"/>
        </record>

        <!-- Catálogos -->
        <record id="codigo_prefectura" model="mz.catalogo">
            <field name="name">Codigo Prefectura</field>
            <field name="descripcion">Codigo Prefectura</field>
        </record>
        

       
        <!-- Tipo de servicios -->
        <record id="codigo_prefectura_items" model="mz.items">
            <field name="name">GADM-PG</field>
            <field name="descripcion">Esta abreviatura se utilizará para generar los códigos únicos.</field>
            <field name="catalogo_id" ref="codigo_prefectura"/>
        </record>
        
    </data>    
</odoo>